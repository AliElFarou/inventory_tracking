<h1>Warehouse: <%= @warehouse.name %></h1>
<p>Description: <%= @warehouse.location %></p>

<h2>Warehouse Items:</h2>

<ul>
    <% @warehouse.warehouse_items.each do |f| %>
        <li><%= @warehouse.item.name %>, <%= @warehouse.item.description %></li>
    <% end %>
</ul>


<%= button_to "Edit Warehouse", edit_warehouse_path, method: :get %>
<%= button_to "Delete Warehouse", warehouse_path, method: :delete %>